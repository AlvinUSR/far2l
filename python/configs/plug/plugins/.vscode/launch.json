{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Attach",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}",
            "port": 5678,
            "host": "localhost"
        },
        {
            "name": "FAR2L",
            "type": "cppdbg",
            "request": "launch",
            "program": "/devel/bin/farg/far2l",
            "stopAtEntry": true,
            "args": [
            ],
            "cwd": "/tmp",
            "environment": [
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                }
            ]
        }
    ]
}